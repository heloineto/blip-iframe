{
  "name": "blip-iframe",
  "version": "0.0.5",
  "description": "A type safe and intuitive API to communicate with the Blip platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "author": "Heloi Neto",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heloineto/blip-iframe.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/heloineto/blip-iframe/issues"
  },
  "homepage": "https://github.com/heloineto/blip-iframe#readme",
  "devDependencies": {
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "iframe-message-proxy": "^1.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true
  },
  "publishConfig": {
    "access": "public"
  }
}
