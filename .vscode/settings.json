{
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.fixAll",
    "source.organizeImports"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    ".nyc_output": true
  },
  "editor.formatOnSave": true,
  "cSpell.words": [
    "clsx",
    "datetimeoffset",
    "Heloi",
    "iframe",
    "Informations",
    "mantine",
    "procurationem",
    "substringof",
    "tsup"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
